{
  "name": "kalabasa.github.io",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Lean Rada",
  "devDependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.5",
    "chokidar-cli": "^1.2.0",
    "concurrently": "^3.6.0",
    "esm": "^3.0.63",
    "glob": "^7.1.2",
    "handlebars": "^4.0.11",
    "html2plaintext": "^2.0.1",
    "lodash": "^4.17.10",
    "marked": "^1.1.1",
    "marked-plaintext": "^0.0.2",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "stylus": "^0.54.5",
    "uglifycss": "^0.0.29"
  },
  "scripts": {
    "clean": "rimraf build/ gen/",
    "build": "node -r esm build.js",
    "data": "node -r esm src/data/genIndex.js && node -r esm src/data/compileData.js",
    "watch": "concurrently 'yarn watch-data' 'yarn watch-site'",
    "watch-data": "chokidar 'src/data/**' -c 'yarn data; yarn build'",
    "watch-site": "chokidar 'src/**' -i 'src/data/**' -c 'yarn build'",
    "serve": "mkdir -p build && browser-sync build -w --no-notify",
    "dev": "concurrently 'yarn build' 'yarn serve' 'yarn watch'"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "axios-extensions": "^3.0.3",
    "barba.js": "^1.0.0",
    "elasticlunr": "^0.9.5",
    "query-string": "^6.1.0"
  }
}
