{
  "plugins": {
    "html": true
  },
  "filters": {
    "comments": true,
    "node-types": {
      "nodeTypes": ["CodeBlock", "BlockQuote"]
    }
  },
  "rules": {
    "apostrophe": true,
    "no-start-duplicated-conjunction": true,
    "max-comma": true,
    "terminology": {
      "defaultTerms": false,
      "terms": "scripts/dev/lint.terms.jsonc"
    }
  }
}
